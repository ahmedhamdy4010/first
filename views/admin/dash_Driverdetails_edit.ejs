      outline: none;
      padding: 2px 5px;
      width: 75%;
      color: #fff;
      text-align: right;
      margin-top: 10px;
    }
    
    .separator {
      margin-top: 20px;
      margin-bottom: 6px;
      border-bottom: solid 1px rgba(151, 151, 151, 0.2);
    }
    
    .actions {
      display: flex;
      justify-content: space-between;
      cursor: pointer;
      
      font-size: 13px;
      font-weight: 600;
      color: rgba(74, 74, 74, 0.4);
      transition: 3.3s;
      
    }
    
    .actions a {
      transition: color 0.2s ease;
    }
    
    .actions a:hover {
      color: rgba(74, 74, 74, 0.5);
    }
    
    /**** green ****/
    .green {
      background-color: #8e44ad;
    }
    
    .orange {
      background-color: #e8893c;
    }
    
    .red {
      background-color: #ce593f;
    }
    
    .black {
      background-color: rgba(0, 0, 0, .8);
    }
    
    .green .card-content,
    .orange .card-content,
    .red .card-content,
    .black .card-content{
      color: #fff;
    }
    
    .green .actions,
    .orange .actions,
    .red .actions,
    .black .actions {
      color: rgba(256, 256, 256, .8);
    }
    
    .green .actions a:hover,
    .orange .actions a:hover,
    .red .actions a:hover,
    .black .actions a:hover {
      color: rgba(256, 256, 256, 1);
    }
    
    .green .separator,
    .orange .separator,
    .red .separator,
    .black .separator{
      border-bottom: solid 1px rgba(256, 256, 256, .4);
    }
    
    
    
    
    
    
    
            </style>
     


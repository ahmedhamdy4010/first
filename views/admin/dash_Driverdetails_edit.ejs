      background-color: #ffffff;
      box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.14), 0 10px 10px -4px rgba(0, 0, 0, 0.09);
      margin-bottom: 25px;
      /* float: left; */
      margin-right: 3rem; 
    
    }
    @media (max-width:767px){
        .cardd {
            margin: 0 auto;
            width: 250px;
        }
    }
    
    @media (max-width:767px){
        .sp_info {
            margin-top: 40px;
            display: flex;
            align-items: center;
            flex-direction: column;
        }
    }
    
    
    .card-content {
      padding: 15px 20px 10px;;
      color: #4a4a4a;
    }
    
    .balance {
      display: flex;
      justify-content: space-between;
    }
    
    .balance-right {
      text-align: right;
    }
    
    .currency {
      font-size: 40px;
      font-weight: 200;
      line-height: 50px;
      margin-top: 20px;
    }
    
    .balance-title {
      font-size: 14px;
    }
    
    .balance-amount {
      font-size: 25px;
